{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ngv-notification": {
      "projectType": "library",
      "root": "projects/ngv-notification",
      "sourceRoot": "projects/ngv-notification/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/ngv-notification/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/ngv-notification/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/ngv-notification/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/ngv-notification/src/test.ts",
            "tsConfig": "projects/ngv-notification/tsconfig.spec.json",
            "karmaConfig": "projects/ngv-notification/karma.conf.js"
          }
        }
      }
    }
  },
  "cli": {
    "analytics": "bcf9ed7b-0bb8-4de0-a215-860e0c689e21"
  }
}
